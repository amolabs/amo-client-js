{
  "name": "amo-client",
  "version": "1.7.1",
  "description": "Javascript library for AMO client",
  "main": "dist/index.js",
  "types": "./src/@types/index.d.ts",
  "engines": {
    "node": ">=10.15"
  },
  "scripts": {
    "types": "tsc --declaration --outDir src/@types --emitDeclarationOnly",
    "build": "babel --extensions=\".ts\" src/*.ts --out-dir dist",
    "test": "jest --runInBand"
  },
  "files": [
    "dist",
    "src",
    "index.d.ts",
    "README.ko.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amolabs/amo-client-js.git"
  },
  "keywords": [
    "AMO",
    "blockchain",
    "rpc",
    "explorer"
  ],
  "author": "Yeon-Hyeong Yang <yang.yh@pentasecurity.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amolabs/amo-client-js/issues"
  },
  "homepage": "https://github.com/amolabs/amo-client-js#readme",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true,
        "isolatedModules": true
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/elliptic": "^6.4.12",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.4",
    "nan": "^2.14.1",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "elliptic": "^6.5.2"
  }
}
